{
  "name": "boadiversao/l5-repository",
  "description": "Laravel 5 - Repositories to  the database layer",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Eduardo Lima Braz",
      "email": "carloseduardolbraz@gmail.com",
      "role": "Software Engineer"
    }
  ],
  "support": {
    "email": "carloseduardolbraz@gmail.com",
    "issues":"https://github.com/thekaduu/l5-repository/issues",
    "source":"https://github.com/thekaduu/l5-repository"
  },
  "require": {
    "illuminate/http": "~5.0",
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/database": "~5.0",
    "illuminate/pagination": "~5.0",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0",
    "prettus/laravel-validation": "1.1.*",
    "fzaninotto/faker": "^1.8@dev"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\" : "tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.2-dev"
    },
    "laravel": {
      "providers": [
        "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "6.4",
    "mockery/mockery": "dev-master"
  }
}
